<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TemplateController">
   <HBox fx:id="header" alignment="CENTER" spacing="15.0" AnchorPane.bottomAnchor="550.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <children>
         <Text onMouseClicked="#homePageClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Home Page">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Text>
         <Text layoutX="483.0" layoutY="40.0" onMouseClicked="#detailedSearchClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Detailed Search">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Text>
         <Text layoutX="506.0" layoutY="40.0" onMouseClicked="#eventFSClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Event-focused Search">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Text>
         <Text layoutX="529.0" layoutY="40.0" onMouseClicked="#putAdClicked" strokeType="OUTSIDE" strokeWidth="0.0" text="Put Ad">
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Text>
         <Button fx:id="loginButton" mnemonicParsing="false" onAction="#loginClicked" text="Login">
            <HBox.margin>
               <Insets left="30.0" />
            </HBox.margin></Button>
         <Button fx:id="signUpButton" mnemonicParsing="false" onAction="#signUpClicked" text="Sign Up" />
         <Text fx:id="userNameText" disable="true" onMouseClicked="#userNameClicked" strokeType="OUTSIDE" strokeWidth="0.0">
            <HBox.margin>
               <Insets left="50.0" />
            </HBox.margin>
            <cursor>
               <Cursor fx:constant="HAND" />
            </cursor></Text>
         <Button fx:id="userButton" mnemonicParsing="false" onAction="#userNameClicked" text="Button">
            <graphic>
               <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../images/userIcon.png" />
                  </image>
               </ImageView>
            </graphic>
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </children>
   </HBox>
   <HBox alignment="CENTER" layoutY="550.0" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="550.0">
      <children>
         <Text fill="#c7bbbb" strokeType="OUTSIDE" strokeWidth="0.0" text="About Us" />
         <Text fill="#c7bbbb" layoutX="418.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Terms of Service and Privacy Policy" />
         <Text fill="#c7bbbb" layoutX="420.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Copyright Â© 2016 Cyken Co." />
      </children>
   </HBox>
</AnchorPane>
